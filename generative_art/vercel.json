{
    "version": 2,
    "builds": [
        {
            "src": "generative_art/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python3.9"
            }
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{ "distDir":"staticfiles_build" }
        }
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "generative_art/wsgi.py"
        }
    ]
}